# Gmail Categorizer Frontend Environment Configuration
# Copy this file to .env and update the values with your own

# API URL - URL of the backend API
REACT_APP_API_URL=http://localhost:5001

# Google Auth callback path
REACT_APP_GOOGLE_AUTH_REDIRECT_PATH=/auth/callback

# Feature flags
REACT_APP_ENABLE_ANALYTICS=false
REACT_APP_ENABLE_MOCK_DATA=false